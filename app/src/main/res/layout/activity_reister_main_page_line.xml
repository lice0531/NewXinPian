<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/child_pg_bg"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical"
    tools:context="android_serialport_api.xingbang.firingdevice.ReisterMainPage_line"
    tools:ignore="MergeRootFrame">
    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay"
        >

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            app:popupTheme="@style/AppTheme.PopupOverlay"
            app:titleTextColor="@color/white" />

    </com.google.android.material.appbar.AppBarLayout>
    <LinearLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/mainBasicPage"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="@dimen/childReisterPageLineHeight"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView
                    android:id="@+id/text_start"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/text_reister_start"
                    style="@style/alltextstyle"/>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="6">

                    <EditText
                        android:id="@+id/entBF2Bit_st"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/childReisterPageEditHeight"
                        android:layout_gravity="center_vertical"
                        android:layout_marginRight="2dp"
                        android:layout_weight="2"
                        android:background="@drawable/translucent"
                        android:gravity="center"
                        android:hint="@string/text_reister_2b"
                        android:maxLength="2"
                        android:numeric="integer"
                        android:selectAllOnFocus="true"
                        style="@style/alltextstyle"

                        />

                    <EditText
                        android:id="@+id/entproduceDate_st"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/childReisterPageEditHeight"
                        android:layout_gravity="center_vertical"
                        android:layout_marginRight="2dp"
                        android:layout_weight="5"
                        android:background="@drawable/translucent"
                        android:gravity="center"
                        android:hint="@string/text_reister_5b"
                        android:maxLength="5"
                        android:numeric="integer"
                        android:selectAllOnFocus="true"
                        style="@style/alltextstyle"

                        />

                    <EditText
                        android:id="@+id/entAT1Bit_st"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/childReisterPageEditHeight"
                        android:layout_gravity="center_vertical"
                        android:layout_marginRight="2dp"
                        android:layout_weight="1"
                        android:background="@drawable/translucent"
                        android:gravity="center"
                        android:hint="@string/text_reister_1b"
                        android:maxLength="1"
                        android:selectAllOnFocus="true"
                        style="@style/alltextstyle"/>

                    <EditText
                        android:id="@+id/entboxNoAndSerial_st"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/childReisterPageEditHeight"
                        android:layout_gravity="center_vertical"
                        android:layout_marginRight="2dp"
                        android:layout_weight="5"
                        android:background="@drawable/translucent"
                        android:gravity="center"
                        android:hint="@string/text_reister_5b"
                        android:maxLength="5"
                        android:numeric="integer"
                        android:selectAllOnFocus="true"
                        style="@style/alltextstyle"/>
                </LinearLayout>

                <Button
                    android:id="@+id/btn_ReisterScanStart_st"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.2"
                    android:background="@drawable/bt_mainpage_style"
                    android:gravity="center"
                    android:text="@string/text_reister_scan"
                    android:textColor="@color/bt_fg_base"
                    style="@style/alltextstyle" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="@dimen/childReisterPageLineHeight"
                android:visibility="gone">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/text_reister_end"
                    style="@style/alltextstyle"

                    />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="6">

                    <EditText
                        android:id="@+id/entBF2Bit_ed"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/childReisterPageEditHeight"
                        android:layout_gravity="center_vertical"
                        android:layout_marginRight="2dp"
                        android:layout_weight="2"
                        android:background="@drawable/translucent"
                        android:gravity="center"
                        android:hint="@string/text_reister_2b"
                        android:maxLength="2"
                        android:numeric="integer"
                        android:selectAllOnFocus="true"
                        style="@style/alltextstyle"

                        />

                    <EditText
                        android:id="@+id/entproduceDate_ed"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/childReisterPageEditHeight"
                        android:layout_gravity="center_vertical"
                        android:layout_marginRight="2dp"
                        android:layout_weight="5"
                        android:background="@drawable/translucent"
                        android:gravity="center"
                        android:hint="@string/text_reister_5b"
                        android:maxLength="5"
                        android:numeric="integer"
                        android:selectAllOnFocus="true"
                        style="@style/alltextstyle" />

                    <EditText
                        android:id="@+id/entAT1Bit_ed"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/childReisterPageEditHeight"
                        android:layout_gravity="center_vertical"
                        android:layout_marginRight="2dp"
                        android:layout_weight="1"
                        android:background="@drawable/translucent"
                        android:gravity="center"
                        android:hint="@string/text_reister_1b"
                        android:maxLength="1"
                        android:selectAllOnFocus="true"
                        style="@style/alltextstyle" />

                    <EditText
                        android:id="@+id/entboxNoAndSerial_ed"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/childReisterPageEditHeight"
                        android:layout_gravity="center_vertical"
                        android:layout_marginRight="2dp"
                        android:layout_weight="5"
                        android:background="@drawable/translucent"
                        android:gravity="center"
                        android:hint="@string/text_reister_5b"
                        android:maxLength="5"
                        android:numeric="integer"
                        android:selectAllOnFocus="true"
                        style="@style/alltextstyle" />
                </LinearLayout>

                <Button
                    android:id="@+id/btn_ReisterScanStart_ed"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.2"
                    android:background="@drawable/bt_mainpage_style"
                    android:gravity="center"
                    android:text="@string/text_reister_scan"
                    android:textColor="@color/bt_fg_base"
                    style="@style/alltextstyle"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ly_setDelay"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:visibility="gone">

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="5dp">

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="45dp"
                        android:layout_marginTop="8dp">

                        <TextView
                            android:id="@+id/textView9"
                            style="@style/allbtntstyle"
                            android:layout_width="0dp"
                            android:layout_height="45dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/bt_mainpage_style"
                            android:text="@string/text_delay_start"
                            />

                        <EditText
                            android:id="@+id/setDelayTime_startDelaytime"
                            android:layout_width="0dp"
                            android:layout_height="35dp"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="20dp"
                            android:layout_weight="1.5"
                            android:background="@drawable/translucent"
                            android:gravity="left"
                            android:hint="@string/text_delay_start"
                            android:maxLength="6"
                            android:numeric="integer"
                            android:padding="5dp"
                            android:textColor="@color/rangle_fc_base"
                            android:textSize="@dimen/fireChildPageLabelFontSize" />

                        <TextView
                            android:id="@+id/textView5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="@string/text_unit_hm"
                            android:textSize="@dimen/childPageEditFontSize" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="55dp"
                        android:layout_marginTop="8dp">

                        <Button
                            android:id="@+id/re_btn_f1"
                            style="@style/allbtntstyle"
                            android:layout_width="0dp"
                            android:layout_height="45dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/bt_mainpage_style"
                            android:text="F1延时" />

                        <EditText
                            android:id="@+id/re_et_f1"
                            android:layout_width="0dp"
                            android:layout_height="35dp"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="20dp"
                            android:layout_weight="1.5"
                            android:background="@drawable/translucent"
                            android:gravity="left"
                            android:hint="F1延时"
                            android:maxLength="6"
                            android:numeric="integer"
                            android:padding="5dp"
                            android:textColor="@color/rangle_fc_base"
                            android:textSize="@dimen/fireChildPageLabelFontSize" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="@string/text_unit_hm"
                            android:textSize="@dimen/childPageEditFontSize" />
                    </TableRow>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="65dp"
                        android:layout_marginTop="8dp">

                        <Button
                            android:id="@+id/re_btn_f2"
                            style="@style/allbtntstyle"
                            android:layout_width="0dp"
                            android:layout_height="45dp"
                            android:layout_marginLeft="5dp"
                            android:layout_marginRight="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/bt_mainpage_style"
                            android:text="F2延时" />

                        <EditText
                            android:id="@+id/re_et_f2"
                            android:layout_width="0dp"
                            android:layout_height="35dp"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="20dp"
                            android:layout_weight="1.5"
                            android:background="@drawable/translucent"
                            android:gravity="left"
                            android:hint="F2延时"
                            android:maxLength="6"
                            android:numeric="integer"
                            android:padding="5dp"
                            android:textColor="@color/rangle_fc_base"
                            android:textSize="@dimen/fireChildPageLabelFontSize" />

                        <TextView
                            android:id="@+id/textView3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:text="@string/text_unit_hm"
                            android:textSize="@dimen/childPageEditFontSize" />
                    </TableRow>
                </TableLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="@dimen/childReisterPageLineHeight"
                android:gravity="center"
                android:visibility="gone">

                <Button
                    android:id="@+id/btn_return"
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:layout_marginRight="4dp"
                    android:background="@drawable/bt_mainpage_style"
                    android:gravity="center"
                    android:text="@string/text_return"
                    android:textColor="@color/bt_fg_base"
                    android:textSize="@dimen/childReisterPageBtnFontSize" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="@dimen/childReisterPageLineHeight"
                android:layout_marginTop="3dp"
                android:gravity="center">

                <Button
                    android:id="@+id/btn_singleReister"
                    style="@style/allbtntstyle"
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:background="@drawable/bt_mainpage_style"
                    android:text="搜索雷管" />
                <Button
                    android:id="@+id/btn_setdelay"
                    style="@style/allbtntstyle"
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/bt_mainpage_style"
                    android:visibility="gone"
                    android:text="@string/text_single_setDelay" />
                <Button
                    android:id="@+id/btn_inputOk"
                    android:visibility="gone"
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/bt_mainpage_style"
                    android:gravity="center"
                    android:text="@string/text_sure"
                    style="@style/allbtntstyle" />
                <Button
                    android:visibility="visible"
                    android:id="@+id/btn_LookHistory"
                    style="@style/allbtntstyle"
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/bt_mainpage_style"
                    android:text="清空" />


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:id="@+id/ly_showData"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_below="@+id/mainBasicPage"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="25dp"
                android:layout_marginTop="5dp">

                <TextView
                    android:id="@+id/txt_currentVolt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/text_reister_vol"
                    android:textSize="18sp"
                    android:textColor="@color/black"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/txt_currentIC"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/text_reister_ele"
                    android:textSize="18sp"
                    android:textColor="@color/black"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/txt_reisteramount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="已注册:"
                    android:textSize="18sp"
                    android:visibility="gone" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="30dp"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/alltextstyle"
                        android:layout_width="55dp"
                        android:layout_height="30dp"
                        android:background="@drawable/textview_border"
                        android:text="@string/text_list_Serial" />

                    <TextView
                        style="@style/alltextstyle"
                        android:layout_width="60dp"
                        android:layout_height="30dp"
                        android:background="@drawable/textview_border"
                        android:text="@string/text_list_kong" />

                    <TextView
                        style="@style/alltextstyle"
                        android:layout_width="70dp"
                        android:layout_height="30dp"
                        android:background="@drawable/textview_border"
                        android:text="@string/text_list_delay" />

                    <LinearLayout
                        android:id="@+id/re_gkm"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/textview_border">
                        <TextView
                            android:id="@+id/text_gkm1"
                            style="@style/alltextstyle"
                            android:gravity="right"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="30dp"
                            android:text="@string/text_list_guan" />
                        <TextView
                            android:id="@+id/text_gkm2"
                            style="@style/alltextstyle"
                            android:gravity="left"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="30dp"
                            android:text="/UID" />
                    </LinearLayout>
                </LinearLayout>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/factory_listView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scrollbarFadeDuration="0"
                    android:scrollbars="none"
                    android:listSelector="@drawable/listview_color_selector"/>
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>