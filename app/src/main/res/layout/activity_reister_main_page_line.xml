                    android:layout_height="@dimen/dp40"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/bt_mainpage_style"
                    android:gravity="center"
                    android:text="@string/text_sure"
                    style="@style/allbtntstyle" />
                <Button
                    android:visibility="gone"
                    android:id="@+id/btn_LookHistory"
                    style="@style/allbtntstyle"
                    android:layout_width="100dp"
                    android:layout_height="@dimen/dp40"
                    android:layout_marginLeft="4dp"
                    android:layout_marginRight="4dp"
                    android:background="@drawable/bt_mainpage_style"
                    android:text="检测历史" />


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:id="@+id/ly_showData"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_below="@+id/mainBasicPage"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="20dp"
                android:layout_marginTop="5dp">

                <TextView
                    android:id="@+id/txt_currentVolt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/text_reister_vol"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/txt_currentIC"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/text_reister_ele"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/txt_reisteramount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="已注册:"
                    android:textSize="18sp"
                    android:visibility="gone" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="30dp"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/alltextstyle"
                        android:layout_width="55dp"
                        android:layout_height="30dp"
                        android:background="@drawable/textview_border"
                        android:text="@string/text_list_Serial" />

                    <TextView
                        style="@style/alltextstyle"
                        android:layout_width="60dp"
                        android:layout_height="30dp"
                        android:background="@drawable/textview_border"
                        android:text="@string/txt_duan" />

                    <TextView
                        style="@style/alltextstyle"
                        android:layout_width="70dp"
                        android:layout_height="30dp"
                        android:background="@drawable/textview_border"
                        android:text="@string/text_list_delay" />

                    <LinearLayout
                        android:id="@+id/re_gkm"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/textview_border">
                        <TextView
                            android:id="@+id/text_gkm1"
                            style="@style/alltextstyle"
                            android:gravity="right|center_vertical"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="30dp"
                            android:text="@string/text_list_guan" />
                        <TextView
                            android:id="@+id/text_gkm2"
                            style="@style/alltextstyle"
                            android:gravity="left|center_vertical"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="30dp"
                            android:text="@string/text_list_xpm" />
                    </LinearLayout>
                </LinearLayout>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/factory_listView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scrollbarFadeDuration="0"
                    android:scrollbars="none"
                    android:listSelector="@drawable/listview_color_selector"/>
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>